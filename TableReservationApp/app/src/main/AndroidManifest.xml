<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tbash.tablereservation">
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TableReservation">
        <activity android:name=".WelcomeScreen"></activity>
        <activity android:name=".PassCodeLogin" />
        <activity android:name=".ReservationList" />
        <activity android:name=".LoginKaryawan" />
        <activity android:name=".UserReservationStatus" />
        <activity android:name=".FormReservation" />
        <activity android:name=".DateTimeReservation" />
        <activity android:name=".TableResevationStatus" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>